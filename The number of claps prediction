]},{"cell_type":"code","metadata":{"id":"TkibQ7zE_guw","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":833},"executionInfo":{"status":"error","timestamp":1598908779548,"user_tz":-120,"elapsed":1715149,"user":{"displayName":"Yuan Feng","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GjIE6d0los4Ou2d4bXiVrYau6JjyEt-Z15eKhpn=s64","userId":"16902521374199050092"}},"outputId":"44c5fecc-9a39-4358-cffa-a2f0155e281b"},"source":["#mlp = Sequential()\n","#embedding_layer=Embedding(input_dim=NUM_WORDS, output_dim=EMBEDDING_DIM,input_length=MAX_TEXT_LENGTH)\n","#gru=Sequential()  \n","results = dict()\n","\n","for EPOCH in [1]:\n","    for BATCH_SIZE in [100]:\n","        for GRU_NUM in [16,32]:\n","            for MLP_DIM1 in [2]:\n","                for MLP_DIM2 in [2]:\n","                  for EMB_DIM in [200]:\n","                    for GRU_DIM3 in [5,8]:\n","                        for COM_DIM4 in [8,16]:\n","                          for COM_DIM5 in [5,8]:\n","                              mlp = Sequential()\n","                              mlp.add(Dense(MLP_DIM1, input_dim=traincolsX.shape[1], activation=\"relu\"))\n","                              mlp.add(Dropout(0.4))\n","                              mlp.add(Dense(MLP_DIM2, activation=\"relu\"))\n","                              embedding_layer=Embedding(input_dim=NUM_WORDS, output_dim=EMB_DIM, input_length=MAX_TEXT_LENGTH)\n","                              gru = Sequential()\n","                              gru.add(embedding_layer)\n","                              gru.add(Dropout(0.4))\n","                              gru.add(GRU(GRU_NUM))\n","                              gru.add(Dropout(0.4))\n","                              gru.add(Dense(GRU_DIM3,activation=\"relu\"))\n","                              combinedInput = concatenate([mlp.output, gru.output])\n","                              x = Dense(COM_DIM4, activation=\"relu\")(combinedInput)\n","                             