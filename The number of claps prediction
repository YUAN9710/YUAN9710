g layer\n","embedding_layer=Embedding(input_dim=NUM_WORDS, \n","                          output_dim=EMBEDDING_DIM, \n","                          input_length=MAX_TEXT_LENGTH\n","                         )\n","# GRU text \n","model1=Sequential()                        \n","model1.add(embedding_layer)\n","model1.add(GRU(NB_HIDDEN))\n","#model1.add(Dense(64, activation=\"relu\"))  #这一层的输入是文本的输出加多个有关参数\n","#model1.add(Dropout(0.2))\n","#model1.add(Dense(30,activation=\"relu\"))\n","model1.add(Dense(1,activation=\"relu\"))   #这一层是最终输出层，结果是点赞数 \n","model1.compile(loss=\"mae\", optimizer=\"adam\", metrics=[\"mae\"])\n","model1.summary()"],"execution_count":null,"outputs":[{"output_type":"stream","text":["Model: \"sequential\"\n","_________________________________________________________________\n","Layer (type)                 Output Shape              Param #   \n","=================================================================\n","embedding (Embedding)        (None, 900, 150)          3000000   \n","_________________________________________________________________\n","gru (GRU)                    (None, 200)               211200    \n","_________________________________________________________________\n","dense (Dense)                (None, 1)                 201       \n","=================================================================\n","Total params: 3,211,401\n","Trainable params: 3,211,401\n","Non-trainable params: 0\n","_________________________________________________________________\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"QYmgVT2DMv-m","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":346},"executionInfo":{"status":"error","timestamp":1598219521470,"user_tz":-120,"elapsed":1767670,"user":{"displayName":"Yuan Feng","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GjIE6d0los4Ou2d4bXiVrYau6JjyEt-Z15eKhpn=s64","userId":"16902521374199050092"}},"outputId":"54cea4dd-bf70-4702-b8be-503c05663d21"},"source":["mode